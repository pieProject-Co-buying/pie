<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pie.pieProject.DAO.IMemberDao">
	<select id="login" resultType="_int">
		select count (*) from member_user
		where id = #{param1} and password = #{param2}
	</select>
	<insert id="join">
		insert into member_user values (mem_seq.nextval,
		#{id},
		#{password},
		#{name},
		#{nickname},
		#{gender},
		#{profile_pic},
		#{email},
		#{phone},
		#{postCode},
		#{address_main},
		#{address_sub},
		#{agreement},
		null
		)
	</insert>
	<insert id="test">
		insert into testDB values (#{param1}, #{param2})
	</insert>
</mapper>

